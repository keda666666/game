#客户端->服务端
.holy_pet_msg {
	name 		0 : string
	id 			1 : integer #物品id
}

#领取奖励
cs_holy_pet_get_reward 6101 {
	request {
		no 			0 : integer #领取第几个奖励
	}
}

#获取抽奖信息
cs_holy_pet_get_info 6102 {
	request {
	}
	response {
		data 		0 : *holy_pet_msg #抽奖信息
		luckLog		1 : *integer #玩家抽取记录
	}
}


#抽奖
cs_holy_pet_luck_draw 6103 {
	request {
	}
	response {
		ret 	0 : boolean #
		no 		1 : integer #抽到了啥
		data 	2 : *holy_pet_msg #抽奖信息
		luckLog	3 : *integer #玩家抽取记录
	}
}